<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="TermioCommunityWindow" parent="GtkApplicationWindow">
        <property name="resizable">true</property>
        <property name="title" translatable="yes">Termio</property>
        <property name="default-width">1280</property>
        <property name="default-height">800</property>
        <!-- <GtkBox> workbench box -->
        <child>
            <object class="GtkBox" id="workbench_box">
                <property name="orientation">vertical</property>
                <property name="homogeneous">false</property>
                <property name="focusable">true</property>
                <style>
                    <class name="workbench-box" />
                </style>
                <!-- <GtkPaned> workspace paned -->
                <child>
                    <object class="GtkPaned" id="workspace_paned">
                        <property name="orientation">horizontal</property>
                        <property name="vexpand">true</property>
                        <property name="hexpand">true</property>
                        <property name="focusable">true</property>
                        <style>
                            <class name="workspace-paned" />
                        </style>
                        <!-- <GtkBox> Left side bar -->
                        <child>
                            <object class="GtkBox" id="left_side_bar">
                                <property name="orientation">horizontal</property>
                                <style>
                                    <class name="left-side-bar" />
                                </style>
                                <!-- <GtkBox> Workspace activity bar box -->
                                <child>
                                    <object class="GtkBox" id="workspace_activity_bar_box">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">false</property>
                                        <property name="vexpand">true</property>
                                        <property name="hexpand">false</property>
                                        <property name="width-request">30</property>
                                        <style>
                                            <class name="workspace-activity-bar-box" />
                                        </style>
                                    </object>
                                </child>
                                <!-- <GtkSeparator> -->
                                <child>
                                    <object class="GtkSeparator"></object>
                                </child>
                                <!-- <GtkBox> Workspace left side bar box -->
                                <child>
                                    <object class="GtkBox" id="workspace_left_side_bar_box">
                                        <property name="orientation">vertical</property>
                                        <property name="width-request">200</property>
                                        <property name="vexpand">true</property>
                                        <property name="hexpand">true</property>
                                        <property name="spacing">5</property>
                                        <style>
                                            <class name="workspace-left-side-bar-box" />
                                        </style>
                                        <!-- <GtkBox> Wrap box of reate new session button -->
                                        <child>
                                            <object class="GtkBox" id="create_new_session_button_wrap_box">
                                                <property name="width-request">10</property>
                                                <property name="height-request">10</property>
                                                <property name="vexpand">false</property>
                                                <property name="hexpand">false</property>
                                                <!-- <WidgetTitleBar> Session management title bar -->
                                                <child>
                                                    <object class="WidgetTitleBar" id="session_management_title_bar">
                                                        <property name="hexpand">true</property>
                                                        <property name="label">SESSION MANAGEMENT</property>
                                                        <property name="label-font-desc">Consolas</property>
                                                        <property name="control-icon-buttons">
                                                            [
                                                                {
                                                                    "icon_type": "Gtk",
                                                                    "icon_name": "list-add-symbolic",
                                                                    "tooltip": "Create a new remote session.",
                                                                    "action_name": "win.new-session-credential"
                                                                },
                                                                {
                                                                    "icon_type": "Gtk",
                                                                    "icon_name": "list-remove-symbolic",
                                                                    "tooltip": "Minimize session management panel.",
                                                                    "action_name": "win.hide-session-management"
                                                                }
                                                            ]
                                                        </property>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="IconButton" id="font_icon_button">
                                                <property name="icon-type">Segoe MDL2</property>
                                                <property name="code">E72E</property>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="IconButton" id="gtk_icon_button">
                                                <property name="icon-type">Gtk</property>
                                                <property name="icon-name">list-add-symbolic</property>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="IconButton" id="svg_icon_button">
                                                <property name="icon-type">Svg</property>
                                                <property name="icon-name">comments-regular</property>
                                            </object>
                                        </child>
                                        <!-- <GtkScrolledWindow> Session management scrolled window -->
                                        <child>
                                            <object class="GtkScrolledWindow" id="session_management_scrolled_window">
                                                <property name="name">session_management_scrolled_window</property>
                                                <property name="hscrollbar-policy">GTK_POLICY_EXTERNAL</property>
                                                <property name="vscrollbar-policy">GTK_POLICY_AUTOMATIC</property>
                                                <property name="margin-top">10</property>
                                                <property name="margin-bottom">10</property>
                                                <property name="margin-start">5</property>
                                                <property name="margin-end">5</property>
                                                <property name="vexpand">true</property>
                                                <property name="hexpand">true</property>
                                                <child>
                                                    <object class="GtkOverlay" id="terminal_emulator_overlay">
                                                        <child>
                                                            <object class="SessionCredentialManagementTree" id="session_credential_management">
                                                                <property name="hexpand">false</property>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </child>
                        <!-- <GtkScrolledWindow> Terminal scrolled window -->
                        <child>
                            <object class="GtkScrolledWindow" id="workspace_terminal_scrolled_window">
                                <property name="name">terminal-scrolled-window</property>
                                <property name="hscrollbar-policy">GTK_POLICY_EXTERNAL</property>
                                <property name="vscrollbar-policy">GTK_POLICY_EXTERNAL</property>
                                <property name="min-content-width">300</property>
                                <property name="min-content-height">200</property>
                                <property name="vexpand">true</property>
                                <property name="hexpand">true</property>
                                <property name="focusable">true</property>
                                <style>
                                    <class name="workspace-terminal-window" />
                                </style>
                                <child>
                                    <object class="NativeTerminalEmulator" id="native_terminal_emulator"></object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
                <!-- <GtkSeparator> -->
                <child>
                    <object class="GtkSeparator"></object>
                </child>
                <!-- <GtkBox> Bottom status bar -->
                <child>
                    <object class="GtkBox" id="bottom_status_bar_box">
                        <property name="orientation">horizontal</property>
                        <property name="homogeneous">false</property>
                        <property name="vexpand">false</property>
                        <property name="hexpand">true</property>
                        <property name="height-request">15</property>
                        <style>
                            <class name="bottom-status-bar-box" />
                        </style>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>